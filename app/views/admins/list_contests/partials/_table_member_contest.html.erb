<div class="row">
  <div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">Daftar Peserta Lomba</h4>
        <table class="table table-hover">
          <thead>
            <tr>
              <th>School</th>
              <th>Code</th>
              <th>Penolong 1</th>
              <th>Penolong 2</th>
              <th>Penolong 3</th>
            </tr>
          </thead>
          <tbody>
            <% member_contests.each do |member_contest| %>
              <tr>
                <td><%= member_contest.school %></td>
                <td><%= member_contest.code %></td>
                <td><%= member_contest.helper_1 %></td>
                <td><%= member_contest.helper_2 %></td>
                <td><%= member_contest.helper_3 %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>