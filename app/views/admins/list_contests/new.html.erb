<% page_name = "#{params[:action].titleize} Mata Lomba" %>
<div class="page-header">
  <h3 class="page-title"> <%= page_name %> </h3>
  <nav aria-label="breadcrumb">
    <ol class="breadcrumb">
      <li class="breadcrumb-item"><a href="#">Forms</a></li>
      <li class="breadcrumb-item active" aria-current="page"><%= page_name %></li>
    </ol>
  </nav>
</div>


<div class="col-12 grid-margin stretch-card">
  <div class="card">
    <div class="card-body">
      <h4 class="card-title"><%= page_name %></h4>
      <%= render partial: 'admins/list_contests/partials/form', locals: { url: admins_list_contests_path, list_contest: @list_contest, method: :post, type_pmrs: @type_pmrs, type_contests: @type_contests, text_submit: 'Create' } %>
    </div>
  </div>
</div>